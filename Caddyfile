http://www.greenitglobe.com:8080, http://greenitglobe.com:8080 {
bind 0.0.0.0
gzip
    root www.greenitglobe.com/en

    git {
        repo git@github.com:0-complexity/www-greenitglobe.git
        branch production
        interval 300
        key ${HOME}/.ssh/id_rsa
        path ../../
        then hugo -s www.greenitglobe.com
    }
}
